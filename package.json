{
    "name": "nestjs-serverless-boilerplate",
    "version": "0.0.1",
    "description": "Î» Nest.js Serverless boilerplate",
    "author": "Hebert Barros <hebertcisco@outlook.com>",
    "private": true,
    "license": "UNLICENSED",
    "main": "dist/main.js",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --config jest.config.js",
        "test:cov": "npm test -- --coverage",
        "test:watch": "npm test -- --watch",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "npm test -- --config jest-e2e.json",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags",
        "dev": "serverless offline start",
        "predev": "npm run build",
        "deploy": "serverless deploy --verbose",
        "start:docs": "node dist/swagger.js",
        "prestart:docs": "npm run build",
        "dev:docs": "nodemon --watch 'src/' --exec 'ts-node src/swagger.ts' -e ts"
    },
    "dependencies": {
        "@nestjs/common": "9.0.11",
        "@nestjs/core": "9.2.1",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "9.2.1",
        "@nestjs/swagger": "^6.1.4",
        "@nestjs/testing": "^9.2.1",
        "@vendia/serverless-express": "^4.10.1",
        "aws-lambda": "^1.0.7",
        "chance": "1.1.9",
        "class-validator": "0.13.2",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "dotenv": "16.0.3",
        "express": "4.18.2",
        "http-status": "1.6.1",
        "nest-shared": "0.4.53",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.6.0",
        "swagger-ui-express": "4.6.0",
        "tsyringe": "4.7.0"
    },
    "devDependencies": {
        "@babel/cli": "7.20.7",
        "@babel/core": "7.20.7",
        "@babel/node": "7.20.7",
        "@babel/preset-env": "7.20.2",
        "@nestjs/cli": "9.1.5",
        "@nestjs/schematics": "9.0.4",
        "@serverless/typescript": "^3.25.0",
        "@types/aws-lambda": "^8.10.109",
        "@types/chance": "1.1.3",
        "@types/cors": "2.8.13",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.15",
        "@types/jest": "29.2.5",
        "@types/node": "18.11.18",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.46.1",
        "@typescript-eslint/parser": "5.47.1",
        "@typescript-eslint/typescript-estree": "5.47.1",
        "eslint": "8.30.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "jest": "29.3.1",
        "nodemon": "^2.0.20",
        "prettier": "2.8.1",
        "serverless-esbuild": "^1.34.0",
        "serverless-offline": "^12.0.3",
        "supertest": "6.3.3",
        "ts-jest": "29.0.1",
        "ts-loader": "9.4.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.1.1",
        "typescript": "4.9.4"
    }
}